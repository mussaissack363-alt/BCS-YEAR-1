using System;

namespace GhostProtocol.Level1
{
    public class WebFluidSynthesis
    {
        public static void Main(string[] args)
        {
            const string LabName = "Empire State University";

            int batchNumber = 101;
            double viscosity = 0.85;
            string chemicalName = "T-90 Polymer";
            bool isStable = true;

            Console.WriteLine($"[{LabName}] Batch {batchNumber}: {chemicalName} has a viscosity of {viscosity}. Stability: {isStable}");
        }
    }
}
